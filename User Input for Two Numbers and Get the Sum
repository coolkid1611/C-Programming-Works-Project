#include <stdio.h>
#include <stdlib.h>

int add(int x, int y){
	return x+y;
}

int main()
{
	int num1, num2, sum;
	printf("Enter the first number: ");
	scanf("%d", &num1);
	printf("Enter the second number: ");
	scanf("%d",&num2);
	
	//Process calling the function to add
	sum = add(num1, num2);
	
	printf("\n\n\nThe sum of two numbers is %d.\n\n", sum);
	
	system("pause");
	
	return 0;
}
